{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "types": ["vitest/globals", "@testing-library/jest-dom"]
  }
}
